{
  "Table": {
    "prefix": "table",
    "body": [
      "\\begin{table}",
      "\t\\centering",
      "\t{\\small",
      "\t\\begin{tabular}[c]{ll}",
      "\t\\toprule",
      "\t\\textbf{$3} & \\textbf{$4} \\\\\\\\ \\midrule",
      "\ta & b \\\\\\\\",
      "\tc & d \\\\\\\\",
      "\t$5",
      "\t\\bottomrule",
      "\t\\end{tabular}",
      "\t}",
      "\t\\caption{$1}",
      "\t\\label{tab:$2}",
      "\\end{table}",
      "$0"
    ],
    "description": "Add a table"
  },
  "Minipage": {
    "prefix": "minipage",
    "body": ["\\begin{minipage}[t]{$0\\linewidth}", "\\end{minipage}%"],
    "description": "Add a table"
  },
  "Frame": {
    "prefix": "frame",
    "body": ["\\begin{frame}", "\t\\frametitle{$1}", "\\end{frame}", "$0"],
    "description": "Add a beamer frame"
  },
  "Figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}",
      "\t\\centering",
      "\t\\includegraphics[width=0.9\\linewidth]{./figures/$1}",
      "\t\\caption{$2}",
      "\t\\label{fig:$3}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Add a figure"
  },
  "WrapFigure": {
    "prefix": "wrapfig",
    "body": [
      "\\begin{wrapfigure}{r}{0.5\\linewidth}",
      "\t\\centering",
      "\t\\includegraphics[width=0.48\\linewidth]{./figures/$1}",
      "\t\\caption{$2}",
      "\t\\label{fig:$3}",
      "\\end{wrapfigure}",
      "$0"
    ],
    "description": "Add a wrapfigure"
  },
  "BeamerColumn": {
    "prefix": "columns",
    "body": [
      "\\begin{columns}[T]",
      "\t\\begin{column}{0.5\\textwidth}",
      "\t\t$1",
      "\t\\end{column}",
      "\t\\begin{column}{0.5\\textwidth}",
      "\t\t$2",
      "\t\\end{column}",
      "\\end{columns}",
      "$0"
    ]
  },
  "Center": {
    "prefix": "center",
    "body": ["\\begin{center}", "\t$0", "\\end{center}"],
    "description": "Add centering environment"
  }
}
