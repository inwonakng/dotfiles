{
  "Table": {
    "prefix": "table",
    "body": [
      "\\begin{table}",
      "\t\\centering",
      "\t{\\small",
      "\t\\begin{tabular}[c]{ll}",
      "\t\\toprule",
      "\t\\textbf{$3} & \\textbf{$4} \\\\\\\\ \\midrule",
      "\ta & b \\\\\\\\",
      "\tc & d \\\\\\\\",
      "\t$5",
      "\t\\bottomrule",
      "\t\\end{tabular}",
      "\t}",
      "\t\\caption{$1}",
      "\t\\label{tab:$2}",
      "\\end{table}",
      "$0"
    ],
    "description": "Add a table"
  },
  "Minipage": {
    "prefix": "minipage",
    "body": ["\\begin{minipage}[t]{$0\\textwidth}", "\\end{minipage}%"],
    "description": "Add a table"
  },
  "Frame": {
    "prefix": "frame",
    "body": ["\\begin{frame}", "\t\\frametitle{$1}", "\\end{frame}", "$0"],
    "description": "Add a beamer frame"
  },
  "Figure": {
    "prefix": "table",
    "body": [
      "\\begin{figure}",
      "\t\\centering",
      "\t\\includegraphics[width=0.9\\textwidth]{./figures/$1}",
      "\t\\caption{$1}",
      "\t\\label{fig:$2}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Add a table"
  }
}
